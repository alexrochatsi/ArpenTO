							<h:form id="formLocal" enctype="multipart/form-data">
								<p:growl id="growl" />
								<div class="EmptyBox20" />
								<div class="container">
									<div class="Container100 Responsive">
										<p:outputLabel value="Nome do local:" />
										<div class="EmptyBox5"></div>
										<div class="container100">
											<p:inputText id="descricao"
												value="#{cadastroLocal.local.descricao}"
												styleClass="Fleft Responsive" style="width: 95%;" />
										</div>
									</div>
									<div class="EmptyBox20" />
									<div class="Fleft Container20 Responsive">
										<p:outputLabel value="Estado:" />
										<div class="EmptyBox5"></div>
										<div class="container100">
											<p:selectOneMenu id="s_ufLocal"
												value="#{cadastroLocal.ufSelecionada}" filter="true"
												filterMatchMode="startsWith" converter="ufConverter"
												effect="fade">
												<f:selectItem itemLabel="SELECIONE O ESTADO"
													itemValue="#{null}" />
												<f:selectItems value="#{cadastroLocal.ufs}" var="uf"
													itemLabel="#{uf.descUF} / #{uf.siglaUF}" itemValue="#{uf}" />
												<p:ajax event="change"
													listener="#{cadastroLocal.habilitaListCidades}" />
											</p:selectOneMenu>
										</div>
									</div>
									<div class="Container70 Responsive" style="margin-left: 2%">
										<p:outputLabel value="Cidade:" />
										<div class="EmptyBox5"></div>
										<div class="container100">
											<p:autoComplete id="cidadeLocal"
												disabled="#{cadastroLocal.disableCidade}"
												forceSelection="true" styleClass="Fleft Responsive"
												minQueryLength="3" size="100"
												placeholder="Cidade de nascimento..."
												completeMethod="#{cadastroLocal.completeMunicipiosIBGE}"
												converter="ibgeUFConverter" var="iUF"
												itemLabel="#{iUF.nomeMunicipio}" itemValue="#{iUF}"
												value="#{cadastroLocal.local.endereco.municipioUF}">
											</p:autoComplete>
										</div>
									</div>
									<div class="EmptyBox20" />
									<div class="Fleft Container30 Responsive">
										<p:outputLabel value="Número:" />
										<div class="EmptyBox5"></div>
										<p:inputText id="numero" styleClass="Wid90"
											value="#{cadastroLocal.local.endereco.numero}" />
									</div>
									<div class="Fleft Container50 Responsive">
										<p:outputLabel value="Endereço:" />
										<div class="EmptyBox5"></div>
										<p:inputText id="logradouro" styleClass="Wid90"
											value="#{cadastroLocal.local.endereco.logradouro}" />
									</div>
									<div class="EmptyBox20" />
									<div class="Fleft Container50 Responsive">
										<p:outputLabel value="Bairro:" />
										<div class="EmptyBox5"></div>
										<p:inputText id="bairro" styleClass="Wid90"
											value="#{cadastroLocal.local.endereco.bairro}" />
									</div>
									<div class="Fleft Container50 Responsive">
										<p:outputLabel value="Complemento:" />
										<div class="EmptyBox5"></div>
										<p:inputText id="complemento" styleClass="Wid90"
											value="#{cadastroLocal.local.endereco.complemento}" />
									</div>
									<div class="EmptyBox20" />
								</div>

								<div class="EmptyBox20" />
								<div class="Wid30">
									<p:remoteCommand name="rc1"
										actionListener="#{cadastroLocal.acaoIncluir}" />
									<p:commandButton type="button"
										update=":formCadastro:localNascimentoTab:localNascimento"
										styleClass="GreenButton Wid60 Fs18 White RaisedButton"
										onclick="rc1()" value="Adicionar" icon="fa fa-save" />
								</div>
							</h:form>
						</p:dialog>
						<p:dialog header="O que deseja fazer?" modal="true"
							resizable="false" widgetVar="dlgAcao" width="70%" height="100%"
							showEffect="puff" hideEffect="clip">
							<h:form id="formAcao" enctype="multipart/form-data">
								<p:growl id="growl" />
								<div class="EmptyBox20" />
								<div class="Fleft Container100 Responsive">
									<div class="Wid30">
										<p:remoteCommand name="rc3"
											actionListener="#{registroNascimento.redirecionaHome}" />
										<p:commandButton type="button"
											styleClass="RedButton Wid60 Fs16 White RaisedButton"
											onclick="rc3()" value="Ir para o Home" icon="fa fa-home" />
									</div>
									<div class="Wid30">
										<p:remoteCommand name="rc2"
											actionListener="#{registroNascimento.redirecionaNovoCadastro}" />
										<p:commandButton type="button"
											styleClass="GreenButton Wid60 Fs16 White RaisedButton"
											onclick="rc2()" value="Novo Registro" icon="fa fa-plus" />
									</div>
									<div class="Wid30">
										<p:remoteCommand name="rc4"
											actionListener="#{registroNascimento.acaoImprimir}" />
										<p:commandButton type="button"
											styleClass="BlueButton Wid60 Fs16 White RaisedButton"
											onclick="rc4()" value="Imprimir" icon="fa fa-print" />
									</div>
								</div>
							</h:form>
						</p:dialog>
						<p:dialog header="Documento Gerado." modal="true"
							resizable="false" widgetVar="dlgImp" width="80%" height="600px"
							showEffect="puff" hideEffect="clip">
							<div class="Container100 Responsive">
								<h:form id="formPDF" enctype="multipart/form-data"
									rendered="#{registroNascimento.pathRelatorio != null}">
									<p:growl id="growl" />
									<p:panel menuTitle="Último documento gerado"
										header="Último documento gerado">
										<div class="EmptyBox20"></div>
										<object id="viewPDF" type="application/pdf"
											data="../#{registroNascimento.pathRelatorio}" height="400px"
											width="100%">
											Seu navegador não mostrou o pdf, <a
												href="../#{registroNascimento.pathRelatorio}">click aqui</a>
											para baixar o documento.
										</object>
									</p:panel>
								</h:form>
							</div>
						</p:dialog>
					</div>
				</div>
			</div>
		</div>
	</ui:define>
</ui:composition>
</html>