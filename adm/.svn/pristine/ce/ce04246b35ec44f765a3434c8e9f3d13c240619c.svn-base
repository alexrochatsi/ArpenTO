<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core" lang="pt-br">

<ui:composition
	template="../resources/template/template-arpen-portal.xhtml">
	<h:head>
		<title>ARPEN - TO</title>
	</h:head>
	<ui:define name="content" id="#{facesContext.externalContext.request}">
		<div class="Container100 Responsive100">
			<div class="Container100">
				<div class="ContainerIndent">
					<div class="Card ShadowEffect HeiAutoOnMobile"
						style="min-height: 392px;">
						<h:form id="formCadastro" enctype="multipart/form-data">
							<p:growl id="growl" autoUpdate="true" />
							<div class="container">
								<p:wizard widgetVar="wiz"
									flowListener="#{registroNascimento.onFlowProcess}"
									backLabel="Voltar" nextLabel="Avançar">
									<p:tab id="dadosCertidaoTab" title="Certidão">
										<p:panel menuTitle="Certidão" id="dadosCertidaoPanel"
											header="Dados da Certidão">
											<div class="EmptyBox20" />
											<div class="Container60 Responsive">
												<p:outputLabel value="Matricula:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputMask id="preMatricula"
														disabled="#{registroNascimento.matriculaInvalida == false}"
														mask="999999999999999999999999999999"
														value="#{registroNascimento.preMatricula}"
														styleClass="Fleft Responsive" required="true"
														placeholder="Informe a matrícula, somente números..."
														requiredMessage="Campo Vazio! Informe a matrícula."
														style="width: 95%;">
														<f:validateLength minimum="30" maximum="30" />
													</p:inputMask>
												</div>
											</div>
											<div class="Container20 Responsive">
												<p:outputLabel value="Dígito Verificador:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputMask id="dvMatricula" mask="99"
														disabled="#{registroNascimento.matriculaInvalida == false}"
														value="#{registroNascimento.dvMatricula}"
														styleClass="Fleft Responsive" required="true"
														requiredMessage="Campo Vazio! Informe o Dígito Verificador."
														style="width: 45%;">
														<f:validateLength minimum="2" maximum="2" />
													</p:inputMask>
												</div>
											</div>
											<div class="Fleft Container20 Responsive">
												<div class="Wid100">
													<p:remoteCommand name="rcValidaMatri"
														actionListener="#{registroNascimento.validaMatricula}" />
													<p:commandButton type="button" style="margin-top: 10%"
														disabled="#{registroNascimento.matriculaInvalida == false}"
														styleClass="GreenButton Wid60 Fs16 White RaisedButton"
														onclick="rcValidaMatri()" value="Validar"
														icon="fa fa-check" />
												</div>
											</div>
											<div class="EmptyBox20" />
											<p:separator />
											<div class="EmptyBox20" />
											<div class="Container100 Responsive">
												<p:outputLabel value="Nome:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputText id="nomeRegistrado"
														disabled="#{registroNascimento.matriculaInvalida}"
														value="#{registroNascimento.registroNascimento.nome}"
														styleClass="Fleft Responsive" required="true"
														placeholder="Informe o nome que esta registrado no cartório..."
														requiredMessage="Campo Vazio! Informe o nome registrado da certidão."
														style="width: 95%;" />
												</div>
											</div>
											<div class="EmptyBox20" />
											<div class="Container25 Responsive">
												<p:outputLabel value="Data de Nascimento:" />
												<div class="EmptyBox5"></div>
												<p:calendar id="dataNascimento" navigator="true"
													disabled="#{registroNascimento.matriculaInvalida}"
													required="true"
													requiredMessage="Informe a Data de Nascimento!"
													locale="pt_BR" placeholder="__/__/____"
													readonlyInput="true" showOn="button"
													value="#{registroNascimento.registroNascimento.dataNascimento}"
													pattern="dd/MM/yyyy" mask="true" yearRange="c-120:c+60">
													<p:ajax event="dateSelect"
														listener="#{registroNascimento.atualizaDataExtenso}"
														update=":formCadastro:dataNascimentoExtenso,:formCadastro:dataRegistroNascimento" />
												</p:calendar>
											</div>
											<div class="Container70 Responsive">
												<p:outputLabel value="Data Nascimento Extenso:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputText id="dataNascimentoExtenso" disabled="true"
														value="#{registroNascimento.registroNascimento.dataNascimentoExtenso}"
														styleClass="Fleft Responsive" required="true"
														placeholder="Data de nascimento por extenso..."
														requiredMessage="Campo Vazio! Informe a data de nascimento por extenso."
														style="width: 95%;">
													</p:inputText>
												</div>
											</div>
											<div class="EmptyBox20" />
											<div class="Container20 Responsive">
												<p:outputLabel value="Data do Registro de Nascimento:" style="font-size: 14px" />
												<div class="EmptyBox5"></div>
												<p:calendar id="dataRegistroNascimento" navigator="true"
													required="true"
													disabled="#{registroNascimento.matriculaInvalida}"
													requiredMessage="Informe a Data do Registro de nascimento!"
													locale="pt_BR" placeholder="__/__/____"
													value="#{registroNascimento.registroNascimento.dataRegistroNascimento}"
													pattern="dd/MM/yyyy" mask="true" yearRange="c-110:c+110"
													mindate="#{registroNascimento.registroNascimento.dataNascimento}" />
											</div>
											<div class="Container20 Responsive">
												<h:outputText value="Hora de Nascimento Ignorada: " />
												<div class="EmptyBox5"></div>
												<p:selectBooleanCheckbox
													disabled="#{registroNascimento.matriculaInvalida}"
													value="#{registroNascimento.registroNascimento.horaNascimentoIgnorada}"
													style="margin-left: 5%">
													<p:ajax update=":formCadastro:hora,:formCadastro:minuto" />
												</p:selectBooleanCheckbox>
											</div>
											<div class="Container20 Responsive">
												<p:outputLabel value="Hora do Nascimento:" />
												<div class="EmptyBox5"></div>
												<p:inputMask id="hora" mask="99"
													disabled="#{registroNascimento.registroNascimento.horaNascimentoIgnorada == true or 
													registroNascimento.matriculaInvalida == true}"
													value="#{registroNascimento.registroNascimento.horaNascimento}"
													styleClass="Fleft Responsive">
													<p:ajax event="blur"
														listener="#{registroNascimento.validaHora}" />
												</p:inputMask>
											</div>
											<div class="Container20 Responsive">
												<p:outputLabel value="Minuto do Nascimento:" />
												<div class="EmptyBox5"></div>
												<p:inputMask id="minuto" mask="99"
													disabled="#{registroNascimento.registroNascimento.horaNascimentoIgnorada == true or
													registroNascimento.matriculaInvalida == true}"
													value="#{registroNascimento.registroNascimento.minutoNascimento}"
													styleClass="Fleft Responsive">
													<p:ajax event="blur"
														listener="#{registroNascimento.validaMinuto}" />
												</p:inputMask>
											</div>
											<div class="Container20 Responsive">
												<p:outputLabel value="Sexo:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="sexo" required="true"
														disabled="#{registroNascimento.matriculaInvalida}"
														requiredMessage="Informe o Sexo!"
														value="#{registroNascimento.registroNascimento.sexo}">
														<f:selectItem value="#{null}" itemLabel="Selecione o sexo" />
														<f:selectItems value="#{registroNascimento.sexos}"
															var="sexos" itemValue="#{sexos}"
															itemLabel="#{sexos.label}" />
														<f:converter converterId="sexoEnumConverter" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="EmptyBox20" />
											<div class="Fleft Container25 Responsive">
												<p:outputLabel value="Estado:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="s_uf"
														disabled="#{registroNascimento.matriculaInvalida}"
														value="#{registroNascimento.ufSelecionada}" filter="true"
														filterMatchMode="startsWith" required="true"
														requiredMessage="Informe o Estado."
														converter="ufConverter" effect="fade">
														<f:selectItem itemLabel="SELECIONE O ESTADO"
															itemValue="#{null}" />
														<f:selectItems value="#{registroNascimento.ufs}" var="uf"
															itemLabel="#{uf.descUF} / #{uf.siglaUF}"
															itemValue="#{uf}" />
														<p:ajax event="change"
															listener="#{registroNascimento.habilitaListCidades}" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="Container70 Responsive">
												<p:outputLabel value="Cidade:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:autoComplete id="municipioNascimento"
														disabled="#{registroNascimento.disableCidade}"
														forceSelection="true" styleClass="Fleft Responsive"
														required="true" minQueryLength="3" size="100"
														requiredMessage="Campo Vazio! Informe a cidade de nascimento."
														placeholder="Cidade de nascimento..."
														completeMethod="#{registroNascimento.completeMunicipiosIBGE}"
														converter="ibgeUFConverter" var="iUF"
														itemLabel="#{iUF.nomeMunicipio}" itemValue="#{iUF}"
														value="#{registroNascimento.registroNascimento.municipioUF}">
													</p:autoComplete>
												</div>
											</div>
											<div class="EmptyBox30" />
										</p:panel>
									</p:tab>
									<p:tab id="localNascimentoTab" title="Nascimento / DNV">
										<p:panel menuTitle="Nascimento / DNV"
											header="Local de Nascimento / DNV" id="localNascimentoPanel">
											<div class="EmptyBox20" />
											<div class="Container20 Responsive">
												<p:outputLabel value="DNV:" />
												<div class="EmptyBox5"></div>
												<p:inputMask id="dnv" mask="9999999999" required="true"
													disabled="#{registroNascimento.disableDNV1}"
													requiredMessage="Informe o DNV."
													value="#{registroNascimento.registroNascimento.dnv}"
													styleClass="Fleft Responsive" />
											</div>
											<div class="Container20 Responsive">
												<p:outputLabel value="DV:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputMask id="dvDNV" mask="9"
														disabled="#{registroNascimento.disableDNV1}"
														value="#{registroNascimento.dvDNV}"
														styleClass="Fleft Responsive" required="true"
														requiredMessage="Campo Vazio! Informe o Dígito Verificador."
														style="width: 45%;">
														<f:validateLength minimum="1" maximum="1" />
													</p:inputMask>
												</div>
											</div>
											<div class="Fleft Container20 Responsive">
												<div class="Wid100">
													<p:remoteCommand name="rcValidaDNV"
														actionListener="#{registroNascimento.validaDNV}" />
													<p:commandButton type="button" style="margin-top: 10%"
														id="btnValida"
														disabled="#{registroNascimento.disableDNV1}"
														styleClass="GreenButton Wid60 Fs16 White RaisedButton"
														onclick="rcValidaDNV()" value="Validar" icon="fa fa-check" />
												</div>
											</div>
											<div class="Container20 Responsive">
												<h:outputText value="DNV Inexistente: " />
												<div class="EmptyBox5"></div>
												<p:selectBooleanCheckbox
													disabled="#{registroNascimento.disableDNV2}"
													value="#{registroNascimento.registroNascimento.dnvInexistente}"
													style="margin-left: 5%">
													<p:ajax listener="#{registroNascimento.DNVInexistente}"
														update="dnv,dvDNV,btnValida,tipoLocalNascimento,localNascimento,localNlistado" />
												</p:selectBooleanCheckbox>
											</div>
											<div class="EmptyBox20" />
											<p:separator />
											<div class="EmptyBox20" />
											<div class="Container30 Responsive">
												<p:outputLabel value="Tipo de Local de Nascimento:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="tipoLocalNascimento" required="true"
														disabled="#{registroNascimento.disableLocal1}"
														requiredMessage="Informe o tipo de Local de Nascimento"
														value="#{registroNascimento.registroNascimento.tipoLocalNascimento}">
														<f:selectItem value="#{null}"
															itemLabel="Selecione o tipo de local" />
														<f:selectItems
															value="#{registroNascimento.tiposLocaisNascimento}"
															var="tLocal" itemValue="#{tLocal}"
															itemLabel="#{tLocal.label}" />
														<f:converter converterId="localNascimentoEnumConverter" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="Container30 Responsive">
												<p:outputLabel value="Local de Nascimento:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="localNascimento" required="true"
														disabled="#{registroNascimento.disableLocal1}"
														filter="true"
														requiredMessage="Informe o tipo de Local de Nascimento"
														value="#{registroNascimento.registroNascimento.localNascimento}">
														<f:selectItem value="#{null}"
															itemLabel="Selecione o local de nascimento" />
														<f:selectItems value="#{registroNascimento.locais}"
															var="local" itemValue="#{local}"
															itemLabel="#{local.descricao}" />
														<f:converter converterId="localConverter" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="Container20 Responsive">
												<h:outputText value="Local Não Listado: " />
												<div class="EmptyBox5"></div>
												<p:selectBooleanCheckbox id="localNlistado"
													disabled="#{registroNascimento.disableLocal1}"
													value="#{registroNascimento.localInexistente}"
													style="margin-left: 5%">
													<p:ajax update="btnNovoLocal"
														listener="#{registroNascimento.localNaoListado}" />
												</p:selectBooleanCheckbox>
											</div>
											<div class="Wid20 Fright" style="margin-top: 2%">
												<p:commandButton type="button" id="btnNovoLocal"
													disabled="#{registroNascimento.disableLocal2}"
													styleClass="GreenButton Wid60 Fs14 White RaisedButton"
													value="Adicionar Local" icon="fa fa-plus-square"
													onclick="PF('dlg1').show();"></p:commandButton>
											</div>
											<div class="EmptyBox30" />
										</p:panel>
									</p:tab>
									<p:tab id="dadosGenitoraTab" title="Genitora">
										<p:panel menuTitle="Genitora" id="dadosGenitoraPanel"
											header="Dados da Genitora">
											<div class="EmptyBox20" />
											<div class="Container100 Responsive">
												<p:outputLabel value="Nome:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputText id="nomeGenitora" required="true"
														requiredMessage="Campo Vazio! Informe o nome da Genitora."
														value="#{registroNascimento.registroNascimento.genitora.nome}"
														styleClass="Fleft Responsive"
														placeholder="Informe o nome..." style="width: 95%;" />
												</div>
											</div>
											<div class="EmptyBox20" />
											<div class="Container50 Responsive">
												<p:outputLabel value="País de Nascimento:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="paisNascimentoGenitora"
														required="true"
														requiredMessage="Selecione o país de nascimento da Genitora."
														onchange="PF('paisNascimentoGenitora').Value(24);"
														filter="true" filterMatchMode="startsWith"
														placeholder="Pais de nascimento..." style="width: 80%"
														value="#{registroNascimento.registroNascimento.genitora.paisNascimento}">
														<f:selectItem value="#{null}"
															itemLabel="Selecione o país..." />
														<f:selectItems value="#{registroNascimento.paisesIBGE()}"
															var="paises" itemValue="#{paises}"
															itemLabel="#{paises.nome}" />
														<f:converter converterId="paisConverter" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="Container50 Responsive">
												<p:outputLabel value="Nacionalidade:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="NascionalidadeGenitora"
														required="true"
														requiredMessage="Selecione a nacionalidade da Genitora."
														onchange="PF('paisNascimentoGenitora').Value(24);"
														filter="true" filterMatchMode="startsWith"
														placeholder="Pais de nascimento..." style="width: 80%"
														value="#{registroNascimento.registroNascimento.genitora.nacionalidade}">
														<f:selectItem value="#{null}"
															itemLabel="Selecione o nacionalidade..." />
														<f:selectItems value="#{registroNascimento.paisesIBGE()}"
															var="paises" itemValue="#{paises}"
															itemLabel="#{paises.nome}" />
														<f:converter converterId="paisConverter" />
													</p:selectOneMenu>
												</div>
											</div>
											<!-- ______________________________________________________ -->
											<div class="EmptyBox20" />
											<div class="Fleft Container30 Responsive">
												<p:outputLabel value="Naturalidade:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:selectOneMenu id="s_ufGenitora"
														disabled="#{registroNascimento.disableSelectNaturalidadeGenitora}"
														value="#{registroNascimento.ufSelecionadaGenitora}"
														filter="true" filterMatchMode="startsWith" required="true"
														requiredMessage="Selecione a naturalidade da Genitora."
														converter="ufConverter" effect="fade">
														<f:selectItem itemLabel="SELECIONE O ESTADO"
															itemValue="#{null}" />
														<f:selectItems value="#{registroNascimento.ufs}" var="uf"
															itemLabel="#{uf.descUF} / #{uf.siglaUF}"
															itemValue="#{uf}" />
														<p:ajax event="change"
															listener="#{registroNascimento.habilitaListCidadesGenitora}" />
													</p:selectOneMenu>
												</div>
											</div>
											<div class="Container70 Responsive">
												<p:outputLabel value="Municipio:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:autoComplete id="naturalidadeGenitora"
														disabled="#{registroNascimento.disableCidadeGenitora}"
														forceSelection="true" styleClass="Fleft Responsive"
														required="true"
														requiredMessage="Campo Vazio! Informe o município da Genitora."
														minQueryLength="3" size="90"
														placeholder="Cidade de nascimento..."
														completeMethod="#{registroNascimento.completeMunicipiosIBGEGenitora}"
														converter="ibgeUFConverter" var="iUF"
														itemLabel="#{iUF.nomeMunicipio}" itemValue="#{iUF}"
														value="#{registroNascimento.registroNascimento.genitora.naturalidade}">
													</p:autoComplete>
												</div>
											</div>
											<div class="EmptyBox20" />
											<div class="Wid30 Fleft" style="margin-top: 2%">
												<p:remoteCommand name="rcMunGenitora"
													actionListener="#{registroNascimento.novoMunicipioGenitora}" />
												<p:commandButton type="button"
													styleClass="RedButton Wid90 Fs16 White RaisedButton"
													onclick="rcMunGenitora()" value="Municipio não listado"
													icon="fa fa-warning" />
											</div>
											<div class="Container70 Responsive">
												<p:outputLabel value="Novo Município:" />
												<div class="EmptyBox5"></div>
												<div class="container100">
													<p:inputText id="novaNaturalidadeGenitora"
														disabled="#{registroNascimento.disableNovoMunicipioGenitora}"
														forceSelection="true" styleClass="Fleft Responsive"
														minQueryLength="3" size="90"
														placeholder="Município de nascimento..."
														value="#{registroNascimento.registroNascimento.genitora.novoMunicipio}">
													</p:inputText>
												</div>
											</div>
											<!-- _______________________________________________________ -->
											<div class="EmptyBox20" />
											<p:fieldset legend="Documento" toggleable="false"
												id="documentosGenitora" toggleSpeed="500">
												<div class="EmptyBox20" />
												<div class="Container30 Responsive">
													<p:outputLabel value="Tipo de Documento:" />
													<div class="EmptyBox5"></div>
													<div class="container100">
														<p:selectOneMenu id="tipoDocumentoGenitora" filter="true"
															required="true"
															requiredMessage="Selecione o tipo de documento da Genitora."
															value="#{registroNascimento.registroNascimento.genitora.documento.tipoDocumento}">
															<f:selectItem value="#{null}"
																itemLabel="Selecione o tipo de doc." />
															<f:selectItems
																value="#{registroNascimento.tiposDocumento}" var="tipo"
																itemValue="#{tipo}" itemLabel="#{tipo.label}" />
															<f:converter converterId="tipoDocumentoEnumConverter" />
															<p:ajax
																update="serieDocumentoGenitora,descricao_genitora,dataEmissao_genitora,s_uf_genitora,s_cartorioGenitora" />
														</p:selectOneMenu>
													</div>
												</div>
												<div class="Container30 Responsive">
													<p:outputLabel value="Número:" />
													<div class="EmptyBox5"></div>
													<p:inputText id="numeroDocumentoGenitora" required="true"
														requiredMessage="Campo Vazio! Informe o número do documento."
														value="#{registroNascimento.registroNascimento.genitora.documento.numero}"
														styleClass="Fleft Responsive" />
												</div>
